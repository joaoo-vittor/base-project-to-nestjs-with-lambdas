{
  "name": "base-project-to-nestjs-with-lambdas",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "keywords": [],
  "author": "",
  "license": "ISC",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "serverless:dev": "npm run build && serverless offline",
    "nest:gm": "nest g module ${npm_config_name}",
    "nest:gm:dr": "nest g module ${npm_config_name} --dry-run",
    "nest:dto": "nest g class ${npm_config_name}/dto --no-spec",
    "nest:dto:dr": "nest g class ${npm_config_name}/dto --dry-run --no-spec",
    "nest:h": "nest g class utils/helpers/${npm_config_name} --no-spec",
    "nest:h:dr": "nest g class utils/helpers/${npm_config_name} --dry-run --no-spec",
    "nest:gc": "nest g controller ${npm_config_name}",
    "nest:gc:dr": "nest g controller ${npm_config_name} --dry-run",
    "nest:gs": "nest g service ${npm_config_name}",
    "nest:gs:dr": "nest g service ${npm_config_name} --dry-run",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch --passWithNoTests --no-cache --runInBand",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "sls:offline": "sls offline",
    "sls:deploy": "sls deploy --verbose"
  },
  "dependencies": {
    "@middy/core": "^3.4.0",
    "@middy/http-json-body-parser": "^3.4.0",
    "@nestjs/common": "^9.2.0",
    "@nestjs/config": "^2.2.0",
    "@nestjs/core": "^9.2.0",
    "@nestjs/platform-express": "^9.2.0",
    "@vendia/serverless-express": "^4.10.1",
    "aws-lambda": "^1.0.7",
    "aws-sdk": "^2.1290.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "jest-junit": "^14.0.1",
    "moment": "^2.29.4",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.5.7",
    "semver": "^7.3.8",
    "serverless": "^3.24.1",
    "serverless-plugin-optimize": "^4.2.1-rc.1",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@golevelup/ts-jest": "^0.3.4",
    "@nestjs/cli": "^9.1.5",
    "@nestjs/schematics": "^9.0.3",
    "@nestjs/testing": "^9.2.0",
    "@serverless/typescript": "^3.0.0",
    "@types/aws-lambda": "^8.10.71",
    "@types/express": "^4.17.14",
    "@types/faker": "^4.1.12",
    "@types/jest": "^29.2.2",
    "@types/node": "^14.14.25",
    "@types/supertest": "^2.0.12",
    "@types/uuid": "^9.0.0",
    "@typescript-eslint/eslint-plugin": "^5.42.1",
    "@typescript-eslint/parser": "^5.42.1",
    "esbuild": "^0.14.11",
    "eslint": "^8.27.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "faker": "^4.1.0",
    "jest": "^29.3.1",
    "json-schema-to-ts": "^1.5.0",
    "serverless-dotenv-plugin": "^4.0.2",
    "serverless-dynamodb-local": "^0.2.40",
    "serverless-esbuild": "^1.23.3",
    "serverless-offline": "^11.3.0",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.1",
    "ts-jest": "^29.0.3",
    "ts-loader": "^9.4.1",
    "ts-node": "^10.4.0",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.1.3",
    "xml-js": "^1.6.11"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s",
      "!<rootDir>/app.*",
      "!<rootDir>/main.ts",
      "!<rootDir>/serverless.ts",
      "!<rootDir>/**/*.module.ts",
      "!<rootDir>/**/*.dto.ts",
      "!<rootDir>/utils/*"
    ],
    "coverageReporters": [
      "json",
      "text",
      "lcov",
      "cobertura",
      "html"
    ],
    "reporters": [
      "jest-junit",
      "default"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  },
  "engines": {
    "node": ">=16.x"
  }
}
